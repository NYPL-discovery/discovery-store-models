        },
        {
          "id": null,
          "la": null,
          "li": "Features excerpts from his personal journals, archival performance footage, and never-before-seen rehearsal recordings, as well as interviews with Robbins himself and over forty others.",
          "pr": "rdfs:label"
        },
        {
          "id": "bf:Note",
          "la": null,
          "li": null,
          "pr": "rdfs:type"
        }
      ],
      "id": "b18064236#1.0001",
